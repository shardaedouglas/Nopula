  align-self: flex-start;
  height: 100%; }

.wp-block-jetpack-layout-grid-column.is-vertically-aligned-center {
  align-self: center; }

.wp-block-jetpack-layout-grid-column.is-vertically-aligned-bottom {
  align-self: flex-end; }

